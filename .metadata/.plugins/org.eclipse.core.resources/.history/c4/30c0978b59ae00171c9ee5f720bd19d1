package com.example.demo.model;

import javax.validation.constraints.NotNull;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor

public class PendudukModel {

	private Integer idPenduduk;
	private String nik;
	
	@NotNull(message = "Tidak Boleh Kosong")
	private Integer jenis_kelamin, is_wni,is_wafat;
	
	@NotNull(message = "Tidak Boleh Kosong")
	private Integer id_keluarga;
	
	@NotNull(message = "Tidak Boleh Kosong")
	private String nama, tempat_lahir, tanggal_lahir, agama, pekerjaan;
	
	@NotNull(message = "Tidak Boleh Kosong")
	private String status_dalam_keluarga;

	@NotNull(message = "Tidak Boleh Kosong")
	private String status_perkawinan, golongan_darah;

	private KeluargaModel keluarga;

}
